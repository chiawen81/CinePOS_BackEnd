{
    "openapi": "3.0.0",
    "info": {
        "title": "CinePOS_BackEnd",
        "description": "CinePOS 後端API",
        "version": "1.0.0"
    },
    "tags": [
        {
            "name": "Staff"
        },
        {
            "name": "Manager"
        },
        {
            "name": "Common"
        }
    ],
    "paths": {
        "/v1/staff/login": {
            "post": {
                "tags": [
                    "Staff"
                ],
                "summary": "登入請求",
                "requestBody": {
                    "description": "資料格式",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginReq"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginRes"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CommonResFailedWrongPassword"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/staff/user/profile": {
            "post": {
                "tags": [
                    "Staff"
                ],
                "summary": "修改會員名字",
                "requestBody": {
                    "description": "資料格式",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/InfoUpdateReq"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/InfoUpdateRes"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CommonResFailedFieldInvalid"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/manager/login": {
            "post": {
                "tags": [
                    "Manager"
                ],
                "summary": "登入請求",
                "requestBody": {
                    "description": "資料格式",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginReq"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginRes"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CommonResFailedNoFound"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/manager/user/profile": {
            "post": {
                "tags": [
                    "Manager"
                ],
                "summary": "修改會員名字",
                "requestBody": {
                    "description": "資料格式",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/InfoUpdateReq"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/InfoUpdateRes"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Failed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CommonResFailedAuthInvalid"
                                }
                            }
                        }
                    }
                }
            }
        },
		"/v1/manager/timetable/list": {
			"get": {
				"operationId": "GetList",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/TimetableResponse"
										},
										{}
									]
								}
							}
						}
					}
				},
				"description": "取得時刻表",
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "startDate",
						"required": true,
						"schema": {
							"format": "double",
							"type": "number"
						}
					},
					{
						"in": "query",
						"name": "endDate",
						"required": true,
						"schema": {
							"format": "double",
							"type": "number"
						}
					}
				]
			}
		},
		"/v1/manager/timetable/create": {
			"post": {
				"operationId": "CreateTimetable",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/TimetableResponse"
										},
										{}
									]
								}
							}
						}
					}
				},
				"description": "新增時刻表",
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/TimetableRequest"
							}
						}
					}
				}
			}
		},
		"/v1/manager/timetable/update": {
			"patch": {
				"operationId": "UpdateTimetable",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/TimetableResponse"
										},
										{}
									]
								}
							}
						}
					}
				},
				"description": "更新時刻表",
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/TimetableRequest"
							}
						}
					}
				}
			}
		},
		"/v1/manager/timetable/{timetableId}": {
			"delete": {
				"operationId": "DeleteItem",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"anyOf": [
										{
											"$ref": "#/components/schemas/TimetableResponse"
										},
										{}
									]
								}
							}
						}
					}
				},
				"description": "刪除時刻表",
				"security": [],
				"parameters": [
					{
						"in": "path",
						"name": "timetableId",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		}
    },
    "components": {
        "schemas": {
            "LoginReq": {
                "required": [
                    "password",
                    "staffId"
                ],
                "properties": {
                    "staffId": {
                        "type": "string",
                        "description": "員工編號",
                        "example": "A0002"
                    },
                    "password": {
                        "type": "string",
                        "description": "密碼",
                        "example": "asd123"
                    }
                }
            },
            "LoginRes": {
                "required": [
                    "code",
                    "data",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "number",
                        "example": 1
                    },
                    "message": {
                        "type": "string",
                        "example": "message"
                    },
                    "data": {
                        "$ref": "#/components/schemas/LoginRes_data"
                    }
                }
            },
            "CommonResFailedWrongPassword": {
                "properties": {
                    "code": {
                        "type": "number",
                        "example": -1
                    },
                    "message": {
                        "type": "string",
                        "example": "密碼錯誤!"
                    }
                }
            },
            "CommonResFailedNoFound": {
                "properties": {
                    "code": {
                        "type": "number",
                        "example": -1
                    },
                    "message": {
                        "type": "string",
                        "example": "查無此人!"
                    }
                }
            },
            "CommonResFailedFieldInvalid": {
                "properties": {
                    "code": {
                        "type": "number",
                        "example": -1
                    },
                    "message": {
                        "type": "string",
                        "example": "欄位驗證錯誤!"
                    }
                }
            },
            "CommonResFailedAuthInvalid": {
                "properties": {
                    "code": {
                        "type": "number",
                        "example": -1
                    },
                    "message": {
                        "type": "string",
                        "example": "請重新登入!"
                    }
                }
            },
            "InfoUpdateReq": {
                "required": [
                    "newName",
                    "staffId"
                ],
                "properties": {
                    "newName": {
                        "type": "string",
                        "description": "名字",
                        "example": "黃小狗"
                    },
                    "staffId": {
                        "type": "string",
                        "description": "員工編號",
                        "example": "A0002"
                    }
                }
            },
            "InfoUpdateRes": {
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "number",
                        "example": 1
                    },
                    "message": {
                        "type": "string",
                        "example": "message"
                    },
                    "data": {
                        "$ref": "#/components/schemas/InfoUpdateRes_data"
                    }
                }
            },
            "LoginRes_data": {
                "type": "object",
                "properties": {
                    "staffId": {
                        "type": "string",
                        "example": "A0001"
                    },
                    "name": {
                        "type": "string",
                        "example": "文文"
                    },
                    "token": {
                        "type": "string",
                        "example": "123234345"
                    }
                }
            },
            "InfoUpdateRes_data": {
                "type": "object",
                "properties": {
                    "staffId": {
                        "type": "string",
                        "example": "A0001"
                    },
                    "newName": {
                        "type": "string",
                        "example": "文文2"
                    }
                }
            }
        }
    }
}